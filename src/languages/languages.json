{
  "languages": [
    {
      "name": "Asciidoc",
      "extensions": [".asciidoc", ".adoc", ".asc"],
      "multiline": { "start": "////", "end": "////" },
      "singleline": "//"
    },
    {
      "name": "AWK",
      "extensions": [".awk"],
      "singleline":"#"
    },
    {
      "name": "C",
      "extensions": [".c", ".h"],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "C++",
      "extensions": [
        ".cpp",
        ".hpp",
        ".c++",
        ".h++",
        ".cc",
        ".hh",
        ".cxx",
        ".hxx",
        ".inl"
      ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "C#",
      "extensions": [ ".cs" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "CMake",
      "extensions": [ ".cmake" ],
      "multiline": { "start": "#[[", "end": "]]" },
      "singleline": "#"
    },
    {
      "name": "Go",
      "extensions": [ ".go" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Groovy",
      "extensions": [ ".groovy", ".gvy", ".gy", ".gsh" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Java",
      "extensions": [ ".java" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Javascript",
      "extensions": [ ".js" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Kotlin",
      "extensions": [ ".kt", ".kts" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Markdown",
      "extensions": [".md", ".mkd", ".mdwn", ".mdown", ".markdown"],
      "multiline": { "start": "<!--", "end": "-->" }
    },
    {
      "name": "Plain Text",
      "filenames": [ ".gitignore", "CODEOWNERS" ],
      "singleline": "#"
    },
    {
      "name": "Python",
      "extensions": [ ".py" ],
      "multiline": { "start": "\"\"\"", "end": "\"\"\"" },
      "singleline": "#"
    },
    {
      "name": "Swift",
      "extensions": [ ".swift" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Typescript",
      "extensions": [ ".ts" ],
      "multiline": { "start": "/*", "end": "*/"},
      "singleline": "//"
    },
    {
      "name": "YAML Ain't Markup Language",
      "extensions": [".yaml", ".yml"],
      "singleline": "#"
    }
  ]
}
