{
  "languages": [
    {
      "name": "Asciidoc",
      "extensions": [".asciidoc", ".adoc", ".asc"],
      "multiline": { "start": "////", "end": "////" },
      "singleline": "//"
    },
    {
      "name": "AWK",
      "extensions": [".awk"],
      "singleline":"#"
    },
    {
      "name": "C",
      "extensions": [ ".c", ".h" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "C++",
      "extensions": [ ".cpp", ".cc", ".cxx", ".hpp", ".hh", ".hxx" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "C#",
      "extensions": [ ".cs" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "CMake",
      "extensions": [ ".cmake" ],
      "multiline": { "start": "#[[", "end": "]]" },
      "singleline": "#"
    },
    {
      "name": "Dart",
      "extensions": [ ".dart" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Go",
      "extensions": [ ".go" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Groovy",
      "extensions": [ ".groovy", ".gvy", ".gy", ".gsh" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Java",
      "extensions": [ ".java" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Javascript",
      "extensions": [ ".js" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Kotlin",
      "extensions": [ ".kt", ".kts" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "LUA",
      "extensions": [ ".lua" ],
      "multiline": { "start": "--[[", "end": "]]" },
      "singleline": "--"
    },
    {
      "name": "Markdown",
      "extensions": [".md", ".mkd", ".mdwn", ".mdown", ".markdown"],
      "multiline": { "start": "<!--", "end": "-->" }
    },
    {
      "name": "Nix",
      "extensions": [ ".nix" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "#"
    },
    {
      "name": "Objective-C",
      "extensions": [ ".m", ".mm" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Plain Text",
      "filenames": [ ".gitattributes", ".gitignore", "CODEOWNERS" ],
      "singleline": "#"
    },
    {
      "name": "Perl",
      "extensions": [ ".pl", ".pm" ],
      "multiline": { "start": "=pod", "end": "=cut" },
      "singleline": "#"
    },
    {
      "name": "PHP",
      "extensions": [ ".php" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Python",
      "extensions": [ ".py" ],
      "multiline": { "start": "\"\"\"", "end": "\"\"\"" },
      "singleline": "#"
    },
    {
      "name": "Ruby",
      "extensions": [ ".rb" ],
      "multiline": { "start": "=begin", "end": "=end" },
      "singleline": "#"
    },
    {
      "name": "Rust",
      "extensions": [ ".rs" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Scala",
      "extensions": [ ".scala" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Shell",
      "extensions": [ ".sh" ],
      "singleline": "#"
    },
    {
      "name": "Swift",
      "extensions": [ ".swift" ],
      "multiline": { "start": "/*", "end": "*/" },
      "singleline": "//"
    },
    {
      "name": "Typescript",
      "extensions": [ ".ts" ],
      "multiline": { "start": "/*", "end": "*/"},
      "singleline": "//"
    },
    {
      "name": "YAML Ain't Markup Language",
      "extensions": [".yaml", ".yml"],
      "singleline": "#"
    }
  ]
}
